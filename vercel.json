{
  "version": 2,
  "builds": [
    { "src": "dist/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "dist/home", "dest": "dist/pages/admin/home.html" },
    { "src": "dist/registrar", "dest": "dist/pages/auth/entidades/registrarEntidades.html" },
    { "src": "dist/actualizar", "dest": "dist/pages/auth/entidades/actualizarEntidades.html" },
    
    { "src": "dist/js/(.*)", "dest": "dist/js/$1" },
    
    { "src": "dist/output.css", "dest": "dist/output.css" },

    
    { "src": "/(.*)", "dest": "dist/pages/auth/login.html" }
  ]
}


