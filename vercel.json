{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build"  }
    
  ],
  "routes": [
    { "src": "/output.css", "dest": "/output.css"},
    { "src": "/", "dest": "/pages/auth/login.html" },
    { "src": "/home", "dest": "/pages/admin/home.html" },
    { "src": "/registrar", "dest": "/pages/auth/entidades/registrarEntidades.html" },
    { "src": "/actualizar", "dest": "/pages/auth/entidades/actualizarEntidades.html" },
    { "src": "/(.*)", "dest": "/pages/auth/login.html" }
  ],
  "outputDirectory": "dist" 
}


