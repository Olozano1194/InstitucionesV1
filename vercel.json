{
  "version": 2,
  "builds": [
    { "src": "dist/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/dist/pages/auth/login.html" },
    { "src": "/home", "dest": "/dist/pages/admin/home.html" },
    { "src": "/registrar", "dest": "/dist/pages/auth/entidades/registrarEntidades.html" },
    { "src": "/actualizar", "dest": "/dist/pages/auth/entidades/actualizarEntidades.html" },
    
    { "src": "/js/(.*)", "dest": "/dist/js/$1" },
    
    { "src": "/output.css", "dest": "/dist/output.css" },

    
    { "src": "/(.*)", "dest": "/dist/pages/auth/login.html" }
  ]
}


