{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/app.js"
    },
    {
      "src": "/output.css",
      "dest": "/src/output.css"
    },
    {
      "src": "/features/(.*)",
      "dest": "/src/features/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/src/pages/$1"
    },
    {
      "src": "/core/(.*)",
      "dest": "/src/core/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/src/js/$1"
    },
    {
      "src": "/shared/(.*)",
      "dest": "/src/shared/$1"
    },
    {
      "src": "/",
      "dest": "/src/pages/auth/login.html"
    }
  ]
}


